[core]
	excludesfile = ~/.gitignore
[color]
	diff = auto
	ui = true
[alias]
   s = status
   ci = commit -m
   p = push
   pu = pull
   l = "log --graph --pretty=\"format:%C(yellow)%h%Cred%d%Creset %s %C(white) %C(cyan)%an%Creset, %C(green)%ar%Creset\""
[push]
	default = current
[pull]
	default = current
